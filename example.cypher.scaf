query GetUser `
MATCH (u:User {id: $id})
RETURN u.name, u
`

setup `
CREATE (u:User  {id: 1, name: "user1"})
CREATE (u2:User {id: 2, name: "user2"})
`

GetUser {
	test "user 1 is named user1" {
		$id: 1

		u.name: "user1"
		u: {id: 1, name: "user1"}

		assert `MATCH (l:Log) RETURN count(l) as c` {
			c: 0
		}
	}

	test "complex primitives" {
		$list: [1, 2, "three", {nested: true}]
		$map: {a: 1, b: [true, false], c: {deep: "val"}}
	}
}
